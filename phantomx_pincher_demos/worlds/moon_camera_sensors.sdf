        https://fuel.gazebosim.org/1.0/AndrejOrsula/models/lunar_rock0
      </uri>
      <pose>0 -0.3 0.05 0 0 0</pose>
      <plugin filename="ignition-gazebo-label-system" name="ignition::gazebo::systems::Label">
